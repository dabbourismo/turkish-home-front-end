/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@dropDownLists/*": [
        "src/app/_models/dropDownLists/*"
      ],
      "@models/*": [
        "src/app/_models/*"
      ],
      "@environments/*": [
        "src/environments/*"
      ],
      "@material/*": [
        "src/app/material/*"
      ],
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}